#
# title template: special formatting used in BaseParamAction
#
template.longformat=<div><label sclass="asg-title-label">{3} of {1}: </label><label sclass="asg-itemname-label">{2}</label></div>
template.shortformat=<div><label sclass="asg-title-label">{3}</label></div>

#
# title template: colorful-styled long query labels
#
template.references=<hbox align="end"><label sclass="asg-title-label">Referencias: </label><label sclass="asg-itemname-label">{2}</label> <label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.usages=<hbox align="end"><label sclass="asg-title-label">Usos: </label><label sclass="asg-itemname-label">{2}</label> <label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.context=<hbox align="end"><label sclass="asg-title-label">Contexto: </label><label sclass="asg-itemname-label">{2}</label> <label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.both=<hbox align="end"><label sclass="asg-title-label">Referencias y Usos: </label><label sclass="asg-itemname-label">{2}</label> <label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.adv=<hbox align="end"><label sclass="asg-title-label">Consulta avanzada {0}</label></hbox>
template.path=<hbox align="end"><label sclass="asg-title-label">Consulta de ruta {0}: </label><label sclass="asg-itemname-label">{2}</label> <label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.mpath=<hbox align="end"><label sclass="asg-title-label">Consulta con m&aacute;scara {0}: </label><label sclass="asg-itemname-label">{2}</label> <label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.canned=<hbox align="end"><label sclass="asg-title-label">Consulta enlatada: </label><label sclass="asg-itemname-label">{0}</label></hbox>
template.drilldown=<hbox align="end"><label sclass="asg-title-label">Examinar a fondo sobre </label><label sclass="asg-itemname-label">{2}</label></hbox>

