#
# title template: special formatting used in BaseParamAction
#
template.longformat=<div><label sclass="asg-title-label">{3}de {1}: </label><label sclass="asg-itemname-label">{2}</label></div>
template.shortformat=<div><label sclass="asg-title-label">{3}</label></div>

#
# title template: colorful-styled long query labels
#
template.references=<hbox align="end"><label sclass="asg-title-label">Referências </label><label sclass="asg-itemname-label">{2}</label> <label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.usages=<hbox align="end"><label sclass="asg-title-label">Usos </label><label sclass="asg-itemname-label">{2}</label><label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.context=<hbox align="end"><label sclass="asg-title-label">Contexto: </label><label sclass="asg-itemname-label">{2}</label><label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.both=<hbox align="end"><label sclass="asg-title-label">Referências e Usos </label><label sclass="asg-itemname-label">{2}</label><label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.adv=<hbox align="end"><label sclass="asg-title-label">Consulta Avançada {0}</label></hbox>
template.path=<hbox align="end"><label sclass="asg-title-label">Consulta de Caminho {0}: </label><label sclass="asg-itemname-label">{2}</label> <label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.mpath=<hbox align="end"><label sclass="asg-title-label">Consulta Camuflada {0}: </label><label sclass="asg-itemname-label">{2}</label> <label sclass="asg-itemtype-label">[{1}]</label></hbox>
template.canned=<hbox align="end"><label sclass="asg-title-label">Consulta Empacotada: </label><label sclass="asg-itemname-label">{0}</label></hbox>
template.drilldown=<hbox align="end"><label sclass="asg-title-label">Detalhamento sobre </label><label sclass="asg-itemname-label">{2}</label></hbox>

#
# title template suffix dev: this will be added after the last label of other templates, IF the current workspace is in DEV
#
template.suffix.dev=<label sclass="asg-label-unapproved-version">Em Progresso</label>

