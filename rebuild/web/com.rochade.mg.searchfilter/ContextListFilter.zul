<window title="${wtb:l('ContextListFilter.title')}" border="normal" width="500px"
	height="420px" maximizable="false" minimizable="false" closable="false"
	apply="com.rochade.mg.searchfilter.ContextListFilterComposer" sclass="asg-modal-window asg-select-context" >
	<borderlayout sclass="asg-borderlayout">
	    <north border="none">
		    <div id="divNorth"/>
	   	</north>	
		<center autoscroll="true" style="border: 0px;">
			
				<listbox id="itemListbox" 
					use="com.rochade.wa.view.table.WABaseTable" 
					tableShape="MG_ItemNameDef"
					viewMenu="" 
					multiple="true"
					headerVisible="true"
					footerVisible="false">
				</listbox>
			
		</center>
		<south height="35px"  class="field1" style="border: none;">
			<hbox pack="right" width="100%" align="center" style="padding: 5px;"> 
				<button id="listContexts" width="120px" disabled="true"  
					label="${wtb:l('btn.List')}" tooltiptext=""/>
				<separator width="10px"/>	
				<button id="cancel" width="120px" 
					label="${wtb:l('btn.Cancel')}" tooltiptext=""/>
			</hbox>
		</south>
	</borderlayout>
</window>
