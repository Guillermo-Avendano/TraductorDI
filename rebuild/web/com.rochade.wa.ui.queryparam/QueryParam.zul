<window apply="com.rochade.wa.ui.queryparam.QueryParamComposer" border="normal" width="855px" height="480px" sclass="ags-query-param">

<borderlayout>
	<north>	
		<hbox width="100%" widths="80%,20%" sclass="field0" >
			<vbox style="padding:5px">
				<div   id="headerTitle" width="100%"/> 
				<label value="${wtb:l('QueryParam.select')}"/>
			</vbox>
			<vbox width="100%" align="end">
				<image id="imgWizard" src="${wtb:getImg('QueryParam.img.wizard')}"  height="${wtb:l('img.height.wizard')}"/>
			</vbox>
		</hbox>
	</north>
    <center sclass="field1" autoscroll="true">
        <div>
			<div style="padding:5px">
		       	<listbox id="lbRunsettings" seltype="single" height="100%"> 
					<listhead sizable="true">
						<listheader width="20%" label="${wtb:l('QueryParam.property')}" />
						<listheader width="80%" label="${wtb:l('QueryParam.value')}" />
					</listhead>
				</listbox>
			</div>
			<hbox width="100%" widths="80%,10%,10%" style="padding:5px">
			    <label value=" "></label>
		        <button width="100px" id="runButton" label="${wtb:l('QueryParam.runButton')}" tooltiptext="${wtb:l('QueryParam.enter')}" />
				<button width="100px" id="cancelButton" label="${wtb:l('QueryParam.cancelButton')}" tooltiptext="${wtb:l('QueryParam.esc')}"  />
			</hbox>	
 		</div>
	</center>
</borderlayout>
</window>