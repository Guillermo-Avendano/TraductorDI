<div use="com.rochade.wa.ui.view.BaseObjectView" sclass="asg-tab-content-div">
	<grid sclass="asg-itemview-grid">
		<columns sclass="asg-itemview-grid-columns">
			<column width="100%" />
		</columns>
		<rows id="textGrid" sclass="asg-itemview-grid-rows">
			<row valign="top" align="left" id="lGDP_RIGHTOF_INFORMATION" sclass="asg-itemview-gridline-label">
				<label value="${wtb:l('label.GDP_SubjectsInformed')}" sclass="asg-attribute-label-bold"/>
			</row>
			<row valign="top" align="left" sclass="asg-itemview-gridline-value">
				<div use="com.rochade.wa.ui.view.TextPropertyView" rows="7"
					attributeNames="GDP_RIGHTOF_INFORMATION"
					id="GDP_RIGHTOF_INFORMATION"
					relatedId="lGDP_RIGHTOF_INFORMATION" />
			</row>
			<row valign="top" align="left" id="lGDP_RIGHTOF_CONSENT" sclass="asg-itemview-gridline-label">
				<label value="${wtb:l('label.GDP_SubjectsConsentObtained')}" sclass="asg-attribute-label-bold"/>
			</row>
			<row valign="top" align="left" sclass="asg-itemview-gridline-value">
				<div use="com.rochade.wa.ui.view.TextPropertyView" rows="7"
					attributeNames="GDP_RIGHTOF_CONSENT"
					id="GDP_RIGHTOF_CONSENT"
					relatedId="lGDP_RIGHTOF_CONSENT" />
			</row>
			<row valign="top" align="left" id="lGDP_RIGHTOF_ERASURE" sclass="asg-itemview-gridline-label">
				<label value="${wtb:l('label.GDP_RectificationAndErasure')}" sclass="asg-attribute-label-bold"/>
