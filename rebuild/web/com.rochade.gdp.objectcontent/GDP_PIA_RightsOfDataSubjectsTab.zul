			<row valign="top" align="left" sclass="asg-itemview-gridline-value">
				<div use="com.rochade.wa.ui.view.TextPropertyView" rows="7"
					attributeNames="GDP_RIGHTOF_ERASURE"
					id="GDP_RIGHTOF_ERASURE"
					relatedId="lGDP_RIGHTOF_ERASURE" />
			</row>
			<row valign="top" align="left" id="lGDP_RIGHTOF_RESTRICTION" sclass="asg-itemview-gridline-label">
				<label value="${wtb:l('label.GDP_RestrictionAndObject')}" sclass="asg-attribute-label-bold"/>
