			<row valign="top" align="left" id="lGDP_RIGHTOF_CONTR_OBLIGATIONS" sclass="asg-itemview-gridline-label">
				<label value="${wtb:l('label.GDP_ObligationsIdentifiedAndContracted')}" sclass="asg-attribute-label-bold"/>
			</row>
			<row valign="top" align="left" sclass="asg-itemview-gridline-value">
				<div use="com.rochade.wa.ui.view.BooleanPropertyView"
					msgKeyPrefix="BooleanPropertyView.NullAsEmpty." 
					nullAllowed="true"
					attributeNames="GDP_RIGHTOF_CONTR_OBLIGATIONS"
					id="GDP_RIGHTOF_CONTR_OBLIGATIONS"
					relatedId="lGDP_RIGHTOF_CONTR_OBLIGATIONS" />
