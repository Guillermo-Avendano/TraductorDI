<div use="com.rochade.wa.ui.view.BaseObjectView" sclass="asg-tab-content-div">
	<grid sclass="asg-itemview-grid">
		<columns sclass="asg-itemview-grid-columns">
			<column width="100%" />
		</columns>
		<rows id="textGrid" sclass="asg-itemview-grid-rows">
			<row id="lGDP_ES_RISK_ACCESS" valign="top" align="left" sclass="asg-itemview-gridline-label">
				<label value="${wtb:l('label.GDP_RiskSourcesIllegitimateAccess')}" sclass="asg-attribute-label-bold" multiline="true"/>
			</row>
			<row valign="top" align="left" sclass="asg-itemview-gridline-value">
				<div use="com.rochade.wa.ui.view.TableLinkPropertyView"
					sclass="asg-linklist" attributeNames="GDP_ES_RISK_ACCESS"
					tableShape="GDP_PIAUsesEvSrcTable" id="GDP_ES_RISK_ACCESS"
					viewMenu="DWR_FilterViewMenu" relatedId="lGDP_ES_RISK_ACCESS"
					addTargetsComponentId="BILinkSearch" footerVisible="false"
					targetsWorkspaceId="$environment.Edit.EIM__EDITABLE_ITEM" />
			</row>
			<row id="lGDP_ES_RISK_MODIFICATION" valign="top" align="left" sclass="asg-itemview-gridline-label">
