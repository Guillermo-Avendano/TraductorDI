<div use="com.rochade.wa.ui.view.BaseObjectView" width="300px" height="100%">
	<div style="background:transparent" height="100%" width="300px">
		<grid style="border: 0px; padding: 0; background: #FFFFFF;" oddRowSclass="myodd">
			<columns style="background: transparent">
				<column width="30%" />
				<column width="70%" />
			</columns>
			<rows id="topGrid" style="background: transparent; ">
				<row valign="top" align="left" style="border: 0px; background: transparent;">
					<cell colspan="2">
						<label value="${wtb:l('Name')}:" sclass="asg-attribute-label-bold" />
					</cell>
				</row>
				<row valign="top" align="left" style="border: 0px; background: transparent;">
					<cell colspan="2">
						<div use="com.rochade.wa.ui.editor.objectinfo.HierarchicalNSView" attributeNames="name" id="RBG_QUALNAME"
							default="Unable to obtain data" showLinkForLastSegment="false" showWholePath="true" />
					</cell>
				</row>

				<row valign="top" align="left" style="border: 0px; background: transparent; padding-top: 10px">
					<cell colspan="2">
						<label value="${wtb:l('rim.attr.DWR_DESCRIPTION')}:" sclass="asg-attribute-label-bold" />
					</cell>
				</row>
				<row valign="top" align="left" style="border: 0px; background: transparent; ">
					<cell colspan="2">
						<div use="com.rochade.wa.ui.view.TextPropertyView" attributeNames="DWR_DESCRIPTION" width="99%"
							id="DESCRIPTION" />
					</cell>
				</row>
				<row valign="top" align="left" style="border: 0px; background: transparent; padding-top: 10px">
					<cell colspan="2">
						<label value="${wtb:l('ER_model')}:" sclass="asg-attribute-label-bold" />
					</cell>
				</row>
				<row valign="top" align="left" style="border: 0px; background: transparent;">
					<cell colspan="2">
						<div use="com.rochade.wa.ui.view.ObjectContentQueryView" 
							query="mpath=_DWR_ERM_SA2ERM!mask=target" tableShape="itemNameDef" id="BTerm_Ref_By" readOnly="true" viewMenu="DWR_FilterViewMenu"
							sclass="asg-linklist" footerVisible="false" />
					</cell>
				</row>

				<row  id="lDWR_STAGE" valign="top" align="left" style="border: 0px; background: transparent; padding-top: 10px">
					<label value="${wtb:l('rim.attr.DWR_STAGE')}:" sclass="asg-attribute-label-bold" />
					<div use="com.rochade.wa.ui.view.TextPropertyView" attributeNames="DWR_STAGE" width="99%" id="DWR_STAGE" relatedId="lDWR_STAGE" hideIfEmpty="view"/>
				</row>
				<row id="lDWR_BTerm_Usage" valign="top" align="left" style="border: 0px; background: transparent; padding-top: 10px">
					<cell colspan="2">
						<label value="${wtb:l('BusinessTermItemView.List.DefinedBy')}:" sclass="asg-attribute-label-bold" />
					</cell>
				</row>
				<row valign="top" align="left" style="border: 0px; background: transparent; ">
					<cell colspan="2">
						<div use="com.rochade.wa.ui.view.ObjectContentQueryView" query="topic=DWR_ItemViewReferencingBTerms"
							tableShape="DWR_BTerm_Usage" id="DWR_BTerm_Usage" readOnly="true" viewMenu="DWR_FilterViewMenu" sclass="asg-linklist" footerVisible="false" relatedId="lDWR_BTerm_Usage" hideIfEmpty="always" />
					</cell>
				</row>
			</rows>
		</grid>
	</div>
</div>