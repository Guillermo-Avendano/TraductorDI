<div use="com.rochade.wa.ui.view.BaseObjectView" width="300px" height="100%">
	<div style="background:transparent" height="100%" width="300px">
		<grid style="border: 0px; padding: 0; background: #FFFFFF;" oddRowSclass="myodd">
			<columns style="background: transparent">
				<column width="30%" />
				<column width="70%" />
			</columns>
			<rows id="topGrid" style="background: transparent; ">
				<row valign="top" align="left" style="border: 0px; background: transparent;">
					<cell colspan="2">
						<label value="${wtb:l('Label.EIM_Definition')}:" sclass="asg-attribute-label-bold" />
					</cell>
				</row>
				<row valign="top" align="left" style="border: 0px; background: transparent; ">
					<cell colspan="2">
						<div use="com.rochade.wa.ui.view.TextPropertyView" attributeNames="DEFINITION" width="99%" id="DEFINITION" />
					</cell>
				</row>
				<row valign="top" align="left" style="border: 0px; background: transparent; padding-top: 10px">
					<cell colspan="2">
						<label value="${wtb:l('Label.EIM_Message')}:" sclass="asg-attribute-label-bold" />
					</cell>
				</row>
				<row valign="top" align="left" style="border: 0px; background: transparent; ">
					<cell colspan="2">
						<div use="com.rochade.wa.ui.view.TextPropertyView" attributeNames="EIM_MESSAGE" width="99%"
							id="MESSAGE" />
					</cell>
				</row>

				<row valign="top" align="left" style="border: 0px; background: transparent; padding-top: 10px">
					<cell colspan="2">
						<label value="${wtb:l('Label.EIM_Annotated_Item')}:" sclass="asg-attribute-label-bold" />
					</cell>
				</row>
				<row valign="top" align="left" style="border: 0px; background: transparent; ">
					<cell colspan="2">
						<div use="com.rochade.wa.ui.view.TableLinkPropertyView" attributeNames="EIM_ANNOTATED_ITEM" 
							tableShape="EIM_Annotated_Item" viewMenu="DWR_FilterViewMenu" sclass="asg-linklist" footerVisible="false" />
					</cell>
				</row>
			</rows>
		</grid>
	</div>
</div>