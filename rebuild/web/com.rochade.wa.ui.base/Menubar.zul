<?xml version="1.0" encoding="UTF-8"?>
<?page title="menubar" contentType="text/html;charset=UTF-8"?>
<?taglib uri="/WEB-INF/tld/web/wtb.dsp.tld" prefix="wtb" ?>
<!-- 
	@author HO 06/02/2010, created
