<?xml version="1.0" encoding="UTF-8"?>
<?page title="Display Metadata Frame" contentType="text/html;charset=UTF-8"?>
<?taglib uri="/WEB-INF/tld/web/wtb.dsp.tld" prefix="wtb" ?>
