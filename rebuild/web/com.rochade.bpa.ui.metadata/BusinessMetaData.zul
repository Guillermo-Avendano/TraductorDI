<zk>
 	<window id="win" border="normal" width="800px;"  sclass="asg-bpa-metadata"
    		apply="com.rochade.bpa.ui.metadata.BusinessMetaDataFrame" 
       		sizable="true"  maximizable="true" minimizable="false" closable="true"  style="background-color: ${wtb:l('asg-atlantic-light-gray')}">
		<caption label="${wtb:l('BP_DisplayBusinessMetaData.lbl')}" style="color: #000000"/>
   		<panel width="100%" style="padding-top: 0px">
     		<panelchildren style="background:transparent">
     			<!-- header -->
     			<div  id="divHeader" />

     			<!-- detail data, defined in other zul file -->
     			<div  id="divDetails" sclass="asg-tab-content-div" style="overflow: auto; max-height: 650px">
     			</div>

	       		<!-- buttons -->
     			<div align="right" style="background:transparent; padding:3px; margin:10px 0px 10px 10px">
           			<button id="btRefresh" label="${wtb:l('BusinessMetadata.button.refresh')}" image=""/>
           			<button id="btOK" label="${wtb:l('BusinessMetadata.button.close')}" image=""/>
           		</div>
			</panelchildren>
		</panel>
	</window>
</zk>