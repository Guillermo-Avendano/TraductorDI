<window border="none" apply="com.rochade.wa.ui.debug.QueriesComposer" sclass="asg-debug-queris">
	 <hbox id="header" style="padding:5px;" width="100%" widths="80%,20%">
		<div id="headerTitle" width="100%"/>
		<hbox width="100%" pack="end"><label id="laCount"/> <label value="${wtb:l('Debug.queries')}" /></hbox>
	 </hbox>
   	 <div width="100%" vflex="1">
	 	 <listbox id="lbQueries" vflex="1" hflex="1">
			<listhead id="lbHead" sizable="true">
				<listheader width="30%" label="${wtb:l('Debug.queryid')}"/>
	 			<listheader width="20%" label="${wtb:l('Debug.querylabel')}"/>
				<listheader width="50%" label="${wtb:l('Debug.querydef')}"/>
	 		</listhead>
	 	 </listbox>
 	 </div>
</window>
